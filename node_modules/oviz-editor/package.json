{
  "name": "oviz-editor",
  "version": "0.1.0",
  "private": true,
  "main": "dist/oviz-editor.umd.js",
  "module": "dist/oviz-editor.esm.js",
  "unpkg": "dist/oviz-editor.min.js",
  "files": ["dist/*"],
  "browser": {
    "./sfc": "src/oviz-editor.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:standalone": "vue-cli-service build --target lib --name oviz-editor-standalone src/index.standalone.js",
    "build:standard": "vue-cli-service build --target lib --name oviz-editor src/index.js",
    "build:toggle": "vue-cli-service build --target lib --name oviz-editor-toggle src/toggle.js",
    "build": "vue-cli-service build --target lib --name oviz-editor src/index.js && vue-cli-service build --no-clean --target lib --name oviz-editor-standalone src/index.standalone.js && vue-cli-service build --no-clean --target lib --name oviz-editor-toggle src/toggle.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.17.1",
    "core-js": "^3.6.5",
    "sanitize-html": "^1.27.4"
  },
  "devDependencies": {
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "@types/sanitize-html": "^1.27.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  }
}
